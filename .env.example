# Environment Variables for LangChain AI SDK Adapter Examples

# DeepSeek API Configuration
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com

# OpenAI API Configuration (alternative)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1

# Other LLM Provider Configurations
# Anthropic
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google
GOOGLE_API_KEY=your_google_api_key_here

# Azure OpenAI
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_ENDPOINT=your_azure_openai_endpoint_here
AZURE_OPENAI_API_VERSION=2024-02-15-preview

# Example Configuration
EXAMPLE_MODEL_NAME=deepseek-chat
EXAMPLE_TEMPERATURE=0.7
EXAMPLE_MAX_TOKENS=1000

# Development Settings
DEBUG=false
LOG_LEVEL=INFO